{
  "name": "micro-reactive",
  "version": "2.6.2",
  "author": "shewulong",
  "license": "MIT",
  "description": "Reactive core based on Function and Proxy",
  "keywords": [
    "reactive",
    "reactivity",
    "proxy",
    "MVVM",
    "v-model",
    "cli"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulongshe/micro-reactive.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --minify --clean && rimraf dist/index.d.mts",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.20.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.23.4"
  }
}
